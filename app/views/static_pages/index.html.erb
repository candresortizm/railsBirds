<h1 class="text-center">Welcome to Rails Birds</h1>
<div class="container-fluid">
  <section class="chirps">
    <% @chirps.each do |ch| %>
      <article class="chirp">
        <ul>
          <li><%= ch.user.first_name %></li>
        </ul>
        <%= ch.body %>
      </article>
    <% end %>
  </section>
</div>
